<div id="<%= dom_id post %>">

<div class="card">
  <div class="card-body d-flex align-items-center">
    <%= image_tag post.image, class: "rounded-circle mr-3", alt: "imagen del post" %>
    <div>
      <h5 class="card-title"><%= post.title %></h5>
      <p class="card-text"><%= post.description %></p>
    </div>
  </div>
</div>

</div>


